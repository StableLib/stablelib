{
    "compilerOptions": {
        "target": "ES2022",
        "module": "node16",
        "strict": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "moduleResolution": "node16",
        "verbatimModuleSyntax": true,
        "newLine": "LF",
        "sourceMap": true,
        "declaration": true,
        "lib": [
            "es2022",
            "dom",
            "scripthost"
        ]
    },
}
